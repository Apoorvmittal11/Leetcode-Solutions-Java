https://leetcode.com/problems/number-of-employees-who-met-the-target/
class Solution {
    public int numberOfEmployeesWhoMetTarget(int[] hours, int t) {
        int c=0;
        for(int i=0;i<hours.length;i++){
            if(hours[i]>=t){
                c++;
            }
        }
        return c;
    }
}
