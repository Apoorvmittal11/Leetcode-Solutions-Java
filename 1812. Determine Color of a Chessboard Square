https://leetcode.com/problems/determine-color-of-a-chessboard-square/
class Solution {
    public boolean squareIsWhite(String s) {
        if((s.charAt(0)=='a'||s.charAt(0)=='c'||s.charAt(0)=='e'||s.charAt(0)=='g')&&
         (s.charAt(1)=='2'||s.charAt(1)=='4'||s.charAt(1)=='6'||s.charAt(1)=='8') ){
            return true;
        }
        if((s.charAt(0)=='b'||s.charAt(0)=='d'||s.charAt(0)=='f'||s.charAt(0)=='h')&&
         (s.charAt(1)=='1'||s.charAt(1)=='3'||s.charAt(1)=='5'||s.charAt(1)=='7') ){
            return true;
        }
        return false;
    }
}
